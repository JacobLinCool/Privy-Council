{
	"private": true,
	"name": "sveltekit-plus",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --ignore-path .gitignore --check **/*.{ts,js,json,yaml,yml,svelte,html,css} && eslint .",
		"format": "prettier --ignore-path .gitignore --write **/*.{ts,js,json,yaml,yml,svelte,html,css}",
		"prisma": "prisma",
		"db:init": "prisma migrate dev --name init",
		"studio": "prisma studio"
	},
	"dependencies": {
		"@prisma/client": "4.15.0"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.4.8",
		"@changesets/cli": "^2.26.1",
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.35.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/kit": "^1.20.2",
		"@tailwindcss/typography": "^0.5.9",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/debug": "^4.1.8",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"autoprefixer": "^10.4.14",
		"daisyui": "^3.1.0",
		"debug": "^4.3.4",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"openai": "^3.3.0",
		"postcss": "^8.4.24",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"prisma": "^4.15.0",
		"prisma-zod-generator": "^0.8.13",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-i18n": "^3.6.0",
		"svelte-markdown": "^0.2.3",
		"sveltekit-jwt": "^0.2.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vitest": "^0.32.0",
		"zod": "^3.21.4"
	},
	"lint-staged": {
		"*.{ts,js,json,yaml,yml,svelte,html,css}": [
			"prettier --write"
		]
	},
	"packageManager": "pnpm@8.6.0"
}
