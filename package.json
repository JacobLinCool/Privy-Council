{
	"private": true,
	"name": "sveltekit-plus",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --ignore-path .gitignore --check **/*.{ts,js,json,yaml,yml,svelte,html,css} && eslint .",
		"format": "prettier --ignore-path .gitignore --write **/*.{ts,js,json,yaml,yml,svelte,html,css}",
		"prisma": "prisma",
		"db:init": "prisma migrate dev --name init",
		"studio": "prisma studio"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.4.8",
		"@changesets/cli": "^2.26.1",
		"@playwright/test": "^1.33.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.18.0",
		"@tailwindcss/typography": "^0.5.9",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"autoprefixer": "^10.4.14",
		"daisyui": "^2.51.6",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"prisma": "^4.14.1",
		"prisma-zod-generator": "^0.8.11",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-i18n": "^3.6.0",
		"sveltekit-jwt": "^0.2.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.1",
		"typescript": "^5.0.4",
		"vite": "^4.3.7",
		"vitest": "^0.31.1",
		"zod": "^3.21.4"
	},
	"lint-staged": {
		"*.{ts,js,json,yaml,yml,svelte,html,css}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@prisma/client": "4.14.1"
	}
}
